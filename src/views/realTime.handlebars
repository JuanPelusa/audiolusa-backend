<header>
  <nav class="container-fluid mainHeader">
    <img class="fluff" src="/images/logo-brand-3d.png" alt="">
    <ul>
      <li>
        <a class="linkTo" href="/">Home</a>
      </li>
      <li>
        <a class="linkTo" href="/realTime">Products</a>
      </li>
    </ul>
  </nav>
</header>
<main>
    <h3 style="text-align: center; font-size:large">all products</h3>
    <table class="container">
        <thead>
          <tr>
            <th>id</th>
            <th>title</th>
            <th>description</th>
            <th>code</th>
            <th>price</th>
            <th>status</th>
            <th>stock</th>
            <th>category</th>
            <th>image</th>
          </tr>
        </thead>
        <tbody>
          {{#each products}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.code}}</td>
                <td>{{this.price}}</td>
                <td>{{this.status}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.image}}</td>
            </tr>
          {{/each}}  
        </tbody>
    </table>
</main>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/socket.js"></script> 